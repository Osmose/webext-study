{

  "manifest_version": 2,
  "name": "WebExtStudy",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["storage"],

  "browser_action": {
    "browser_style": true,
    "default_icon": "icon.svg",
    "default_title": "Check it out",
    "default_popup": "popup.html"
  }
}
